    <div class="col-xs-3">      
        <div class = "jumbotron">
          <div class="row">
            <h3>Links</h3>
            <h3><%= link_to " Popular Musings ", popular_musings_path, :class=> "btn btn-default" %></h3>
            <h3><%= link_to " Top Musings ", top_musings_path, :class=> "btn btn-default" %></h3>
            <h3><%= link_to " Current Competition ", competition_show_path(:id => 2), :class => "btn btn-default"%></h3>
