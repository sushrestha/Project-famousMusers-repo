<h1> Musers </h1>
<% @musers.each do |muser| %>
	<p> <%= link_to muser.name, muser %>   </p>
	<%= form_for(current_muser.active_subscribes.build) do |f| %>
  	<div><%= hidden_field_tag :followed_id, muser.id %></div>
  	<%= f.submit "Follow", class: "btn btn-primary" %>
<% end %>
<%end%>


