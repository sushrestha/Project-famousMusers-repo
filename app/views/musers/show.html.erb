
<h1><%= @muser.email %></h1>
<p>isModerator:<%= @muser.isModerator %>|</p>
<p>id:<%= @muser.id %>|</p>
<p>email:<%= @muser.email %>|</p>


<% if @muser.musings.any? %>
<h3>Musings: ( <%= @muser.musings.count %> )</h3>
<ol class="microposts">
  <% @muser.musings.each do |musing| %>
	  <li id="micropost-<%= musing.id %>">
		  <span class="title"><%= musing.title %></span>
		  <span class="content"><%= musing.content %>
		    [<%= link_to " Show ", musing_path(musing)%>|<%= link_to " Edit ", edit_musing_path(musing)%>|<%= link_to " Delete ", musing, method: :delete, data: { confirm: 'Are you sure you want to delete?'}%>]
		  </span>
	  </li>
  <% end %>
 </ol>
<% end %>

<p><%= link_to "Create New Musing", new_musing_path %></p>
<p>
  <%= link_to "Log out", logout_path, method: "delete" %>
</p>
  